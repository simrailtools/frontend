{
  "name": "sit-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@10.29.2",
  "scripts": {
    "dev": "vite",
    "build": "openapi-ts && tsc -b && vite build",
    "preview": "tsc -b && vite build && vite preview",
    "lint": "biome lint && eslint .",
    "format": "biome check --fix && eslint . --fix",
    "ci": "biome ci && eslint . --quiet --no-color && pnpm run build",
    "generate:api": "openapi-ts",
    "generate:api:local": "cross-env API_BASE_URL=http://localhost:8081 openapi-ts",
    "generate:proto": "pbjs -t static-module -w es6 --force-long --no-create --no-service --null-semantics -o ./src/api/proto/bundle.js ./.proto/*.proto && pbts -o ./src/api/proto/bundle.d.ts ./src/api/proto/bundle.js"
  },
  "dependencies": {
    "@maplibre/maplibre-gl-style-spec": "^24.4.1",
    "@nats-io/nats-core": "^3.3.0",
    "@tanstack/react-devtools": "^0.9.5",
    "@tanstack/react-pacer": "^0.19.4",
    "@tanstack/react-pacer-devtools": "^0.5.2",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.159.5",
    "@tanstack/react-router-devtools": "^1.159.5",
    "@tanstack/router-plugin": "^1.159.5",
    "@yornaath/batshit": "^0.14.0",
    "clsx": "^2.1.1",
    "fast-equals": "^6.0.0",
    "long": "^5.3.2",
    "luxon": "^3.7.2",
    "maplibre-gl": "^5.18.0",
    "protobufjs": "^8.0.0",
    "react": "^19.2.4",
    "react-clock": "^6.0.0",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "react-map-gl": "^8.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@eslint/js": "^9.39.2",
    "@hey-api/openapi-ts": "^0.92.3",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.5.1",
    "@types/geojson": "^7946.0.16",
    "@types/luxon": "^3.7.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-refresh": "^0.5.0",
    "globals": "^17.3.0",
    "protobufjs-cli": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.1.0"
  },
  "pnpm": {
    "overrides": {
      "glob": "^13.0.1"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "protobufjs": "8"
      }
    }
  }
}

{
  "name": "sit-web",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "build": "openapi-ts && tsc -b && vite build",
    "lint": "biome lint && eslint .",
    "format": "biome check --fix && eslint . --fix",
    "ci": "biome ci && eslint . --quiet --no-color && pnpm run build",
    "generate:api": "openapi-ts",
    "generate:api:local": "cross-env API_BASE_URL=http://localhost:8081 openapi-ts",
    "generate:proto": "pbjs -t static-module -w es6 --force-long --no-create --no-service --null-semantics -o ./src/api/proto/bundle.js ./.proto/*.proto && pbts -o ./src/api/proto/bundle.d.ts ./src/api/proto/bundle.js"
  },
  "dependencies": {
    "@nats-io/nats-core": "^3.3.0",
    "@tanstack/react-devtools": "^0.9.4",
    "@tanstack/react-pacer": "^0.19.4",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.158.4",
    "@tanstack/react-router-devtools": "^1.158.4",
    "@tanstack/router-plugin": "^1.158.4",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "long": "^5.3.2",
    "luxon": "^3.7.2",
    "protobufjs": "^8.0.0",
    "react": "^19.2.4",
    "react-clock": "^6.0.0",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-leaflet-drift-marker": "^4.0.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@eslint/js": "^9.39.2",
    "@hey-api/openapi-ts": "^0.80.18",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.5.0",
    "@types/leaflet": "^1.9.21",
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.19.9",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "protobufjs-cli": "^2.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5"
  }
}
